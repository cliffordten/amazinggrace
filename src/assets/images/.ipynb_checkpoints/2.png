    "import os\n",
    "\n",
    "dirre = [di for di in os.listdir() if di != \"Untitled.ipynb\" and '.ipynb_checkpoints']\n",
    "print(dirre)\n",
    "\n",
    "for di in dirre:\n",
    "    for count, filename in enumerate((os.listdir(di))): \n",
    "        dst =str(count) + \".jpg\"\n",
    "        src =di+'/'+ filename \n",
    "        dst =di+'/'+ dst \n",
    "          \n",
    "        # rename() function will \n",
    "        # rename all the files \n",
    "        os.rename(src, dst)"
